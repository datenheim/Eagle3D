  union{
    //// Plastic Body
    box{<-(L/2),0,-(W/2)><(L/2),(H-(L_Width/2)),(W/2)> pigment{Black}}

    //// Metal Frame on top of body
    difference{
      box{<-(L/2),(H-(L_Width/2)),-(W/2)><(L/2),H,(W/2)>}  // Metal Plate
      cylinder { <0,H/2,0> <0,H+1,0> (B_Dia/2)+0.15}       // Relief around Button
      texture{pigment{Gray70} finish{F_MetalA ambient (0.1 * global_ambient_mul)}}  // Metallic Color and Finish
    }

    //// Button
    object{
      Round_Cylinder_Union(<0,(H-(L_Width/2)),0>,<0,B_Height,0>, B_Dia/2, (B_Dia/2)/10)            // Main Button Component
      texture {pigment{Color} finish{phong 0.4 reflection{0.2} ambient(0.1 * global_ambient_mul)}} // Button Color, and Finish
    }

    //// Rivets
    #local Rivet_Shape=object{cylinder{<0,H,0><0,H+0.3,0>B_Dia/10 pigment{Black}}} // Plastic rivet construction
    union{Rivet_Shape translate<-((L/2)-(B_Dia/5)),0, ((W/2)-(B_Dia/5))>}	 // Rivet 1 - Upper Left
    union{Rivet_Shape translate< ((L/2)-(B_Dia/5)),0, ((W/2)-(B_Dia/5))>}  // Rivet 2 - Upper Right
    union{Rivet_Shape translate<-((L/2)-(B_Dia/5)),0,-((W/2)-(B_Dia/5))>}  // Rivet 3 - Lower Left
    union{Rivet_Shape translate< ((L/2)-(B_Dia/5)),0,-((W/2)-(B_Dia/5))>}  // Rivet 4 - Lower Right

    //// Leads
    // Lead construction
    #local Lead_Shape = union {
      box { <L/3,(H/3),-L_Width> <0,(H/3)+(L_Width/2),L_Width>}  // Horizontal Lead segment - Upper
      #if(SMD=1)
        box { <L/3,(0),-L_Width> <0,(L_Width/2),L_Width> translate<(L-0.3),0,0>}  // Horizontal Lead segment - Lower -SMD
      #end
      box { <-L_Width/2,-1,-L_Width> <L_Width/2,H/3,L_Width>}    // Vertical Lead segment
      cylinder { <-0,H/3,-L_Width>  <0,H/3,L_Width> L_Width/2}   // Radius on top of Lead
      texture{pigment{Gray70} finish{F_MetalA ambient (0.1 * global_ambient_mul)}} // Metallic Color (Lead Color)
    }
    union { Lead_Shape translate                  <-Lead_X,0, Lead_Y>} // Lead 1 - Upper Left
    union { Lead_Shape rotate <0,180,0> translate < Lead_X,0, Lead_Y>} // Lead 2 - Upper Right
    union { Lead_Shape translate                  <-Lead_X,0,-Lead_Y>} // Lead 3 - Lower Left
    union { Lead_Shape rotate <0,180,0> translate < Lead_X,0,-Lead_Y>} // Lead 4 - Lower Right

    //ESD Lead - Bottom Center
    #if(Lead_Y_ESD != 0)
      union {
        box { <-L_Width*2,(H-(L_Width/2)),0><L_Width*2,H-0.001,L/3>}                 // Horizontal Lead segment - Upper
        #if(SMD=1)
          box { <L/3,(0),-L_Width> <0,(L_Width/2),L_Width> translate<(L-0.3),0,0>}   // Horizontal Lead segment - Lower -SMD ***FIXME***
        #end
        cylinder { <-L_Width*2,(H-(L_Width/2)),0> <L_Width*2,(H-(L_Width/2)),0> L_Width/2} // Radius on top of Lead
        box { <-L_Width*2,H/2,-L_Width/2> <L_Width*2,(H-(L_Width/2)),L_Width/2>}     // Vertical Lead segment - Upper
        box { <-L_Width,-1,-L_Width/2><L_Width,H/2,L_Width/2>}                       // Vertical Lead segment - Lower
        translate <0,0,Lead_Y_ESD>                                                   // Position Lead at 0, and Y location.
        texture{pigment{Gray70} finish{F_MetalA ambient (0.1 * global_ambient_mul)}} // Metallic Color (Lead Color)
      }
    #end
  }
#end


